
    &axi_ethernet_0 {  
        local-mac-address = [00 0a 35 00 01 22];  
        phy-handle = <&extphy0>;  
        xlnx,has-mdio = <0x1>;  
        phy-mode = "rgmii-rxid";  
        mdio {  
            #address-cells = <1>;  
            #size-cells = <0>;  
            extphy0: phy@0 {  
                compatible = "marvell,88e1510";  
                device_type = "ethernet-phy";  
                reg = <0>;  
            };  
        };  
    };  
      
